{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "indent": [1, 4],
        "no-underscore-dangle": 0,
        "import/extensions": "off",
        "no-console": "off",
        "no-undef": "off"
    }
}